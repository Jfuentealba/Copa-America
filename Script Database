CREATE TABLE PARTIDO(
    ID INT PRIMARY KEY,
    ID_DS1 INT,
    ID_DS2 INT,
    ID_E INT,
    CONSTRAINT PK_NOMBRETABLA_NOMBREPK PRIMARY KEY (ID)
);
CREATE TABLE EVENTOS(
    ID INT PRIMARY KEY,
    ID_P INT,
    ID_J1 INT,
    ID_J2 INT,
    ID_A INT,
    ID_TE INT,
    ID_S INT,
    MINUTO DATE
);
CREATE TABLE T_EVENTO(
    ID INT PRIMARY KEY,
    NOM VARCHAR(40)
);
CREATE TABLE SANCION(
    ID INT PRIMARY KEY,
    NOM VARCHAR(20)
);
CREATE TABLE D_SELECCION(
    ID INT PRIMARY KEY,
    ID_F INT,
    ID_S INT
);
CREATE TABLE F_ARBITRO(
    ID INT PRIMARY KEY,
    ID_A INT,
    ID_TA INT,
    ID_P INT
);
CREATE TABLE T_ARBITRO(
    ID INT PRIMARY KEY,
    NOM VARCHAR(40)
);
CREATE TABLE ARBITRO(
    ID INT PRIMARY KEY,
    NOM VARCHAR(40)
);
CREATE TABLE ESTADIO(
    ID INT PRIMARY KEY,
    NOM VARCHAR(40),
    CIUDAD VARCHAR(40)
);
CREATE TABLE FORMACIONES(
    ID INT PRIMARY KEY,
    NCENTRO INT,
    NDEFENSA INT,
    NDELANTERO INT
);
CREATE TABLE PAIS(
    ID INT PRIMARY KEY,
    NOM VARCHAR(40)
);
CREATE TABLE SELECCION(
    ID INT PRIMARY KEY,
    ID_P INT,
    ID_FP INT
);
CREATE TABLE F_PERSONAL(
    ID INT PRIMARY KEY,
    ID_TP INT,
    ID_P INT
);
CREATE TABLE T_PERSONAL(
    ID INT PRIMARY KEY,
    NOM VARCHAR(30)
);
CREATE TABLE PAIS(
    ID INT PRIMARY KEY,
    NOM VARCHAR(30)
);
CREATE TABLE CLUB(
    ID INT PRIMARY KEY,
	NOM VARCHAR(30),
    ID_P INT
);
CREATE TABLE JUGADORES(
    ID INT PRIMARY KEY,
    NOM VARCHAR(30),
    ID_S INT,
    ID_J INT
);
CREATE TABLE T_JUGADOR(
    ID INT PRIMARY KEY,
    NOM VARCHAR(30),
    CONSTRAINT PK_TJUGADOR_ID PRIMARY KEY(ID)
);

